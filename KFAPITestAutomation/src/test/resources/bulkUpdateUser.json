{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "array",
    "title": "The Root Schema",
    "description": "The root schema comprises the entire JSON document.",
    "items": {
        "$id": "#/items",
        "type": "object",
        "title": "The Items Schema",
        "description": "An explanation about the purpose of this instance.",
        "default": {},
        "examples": [
            {
                "LastName": "TesterOne",
                "Name": "Smoke TesterOne",
                "_id": "UsnqLZOC7iYY",
                "_modified_at": "2020-04-02T14:51:22Z"
            }
        ],
        "required": [
            "LastName",
            "Name",
            "_id",
            "_modified_at"
        ],
        "properties": {
            "LastName": {
                "$id": "#/items/properties/LastName",
                "type": "string",
                "title": "The Lastname Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "TesterOne"
                ]
            },
            "Name": {
                "$id": "#/items/properties/Name",
                "type": "string",
                "title": "The Name Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "Smoke TesterOne"
                ]
            },
            "_id": {
                "$id": "#/items/properties/_id",
                "type": "string",
                "title": "The _id Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "UsnqLZOC7iYY"
                ]
            },
            "_modified_at": {
                "$id": "#/items/properties/_modified_at",
                "type": "string",
                "title": "The _modified_at Schema",
                "description": "An explanation about the purpose of this instance.",
                "default": "",
                "examples": [
                    "2020-04-02T14:51:22Z"
                ]
            }
        }
    }
}